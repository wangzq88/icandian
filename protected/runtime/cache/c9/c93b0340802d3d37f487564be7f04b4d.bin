	ordering_time = shop.ordering_time.split(' ');
	//显示现在订餐时间
	if (now <= tmp[1]) {//如果现在处于上午的营业时间，显示上午的订餐时间
		now_ordering_time = ordering_time[0];
	} else {
		now_ordering_time = ordering_time[1];
	}
	var show = false;
	for(var i = 0;i < food_list.length; i++) 
	{
		//处于上午营业时间段和处于下午营业时间段
