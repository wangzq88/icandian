function checkCronAction() {
	var shop = {"shop_opening_hours":"09:30-14:00 14:00-20:30","flag":1,"ordering_time":'{"1":"10:00-11:00","2":"17:00-18:00"}'};
	var today = new Date();
	var h = today.getHours();
	var m = today.getMinutes();
	// add a zero in front of numbers<10
	h = checkTime(h);
	m = checkTime(m);
	now = h + ":" + m;
	//上午和下午的营业时间
	list = shop.shop_opening_hours.split(' ');
	//上午的营业时间段
	var tmp = list[0].split('-');
	//下午的营业时间
	var tmp1 = list[1].split('-');
